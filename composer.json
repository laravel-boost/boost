{
    "name": "laravel-boost/boost",
    "description": "A Laravel Front End manager.",
    "keywords": [
        "laravel",
        "boost"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.35"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.10",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Boost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boost\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
        "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [ 
                "\\Boost\\BoostServiceProvider"
            ],
            "aliases": {
                "Boost": "\\Boost\\Boost"
            }   
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}
